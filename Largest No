class Largest {
      public static void main(final String[] args) {
            final int a = 80, b = 80;
            if (a > b) {
                  System.out.println("the largest number." + a);
            }
            if (a < b) {
                  System.out.println("the largest number." + b);
            } else
                  System.out.println("both are equal");
      }
}
